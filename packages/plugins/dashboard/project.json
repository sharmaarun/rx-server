{
  "name": "plugins-dashboard",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugins/dashboard/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/plugins/dashboard",
        "main": "packages/plugins/dashboard/src/index.ts",
        "tsConfig": "packages/plugins/dashboard/tsconfig.lib.json",
        "assets": [
          "packages/plugins/dashboard/*.md",
          "packages/plugins/dashboard/admin/**/*.js",
          "packages/plugins/dashboard/admin/**/*.html",
          "packages/plugins/dashboard/admin/**/*.css",
          "packages/plugins/dashboard/admin/**/*.svg"
        ]
      },
      "configurations": {
        "watch": {
          "watch": true
        }
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs plugins-dashboard {args.ver} {args.tag}"
      },
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/plugins/dashboard/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/plugins/dashboard/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}